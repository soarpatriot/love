<!--miniprogram/pages/explain/list.wxml-->
<view class="container">
  <view class="canvas-module" animation="{{animationData}}">
    <canvas canvas-id="canvas" class="canvas" style="width: 100%; height: 600rpx;background:#fff;;"/>
  </view>
  <view class="analysis">
    <view class="tips">Comprehensiveness</view>
    <view class="total">Total Score: {{totalScore}}</view>
    <view class="tips">Describes</view>
    <view class="comprohesive">
      <view class="progress-box">
        <view class="catetory">Openness</view>
        <progress class="ability" color="#99ff33" percent="{{features.openness.weight}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">Emotion</view>
        <progress class="ability" color="#FF6A6A" percent="{{features.emotion.weight}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">Cooperation</view>
        <progress class="ability" color="#10AEFF" percent="{{features.cooperation.weight}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">Belief</view>
        <progress class="ability" color="#FFB90F" percent="{{features.belief.weight}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">Potential</view>
        <progress class="ability" color="#003366" percent="{{features.potential.weight}}" active stroke-width="6" />
      </view>
    </view>
    <view class="tips">Suggessions</view>
    <view class="suggession">{{suggession}}</view>
  </view>

</view>
