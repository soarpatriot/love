<!--miniprogram/pages/explain/list.wxml-->
<view class="container">
  <view class="canvas-module" animation="{{animationData}}">
    <canvas canvas-id="canvas" class="canvas" style="width: 100%; height: 600rpx;background:#fff;;"/>
  </view>
  <view class="analysis">
    <view class="tips">综合</view>
  
    <view class="tips">单项</view>
    <view class="comprohesive">
      <view class="progress-box">
        <view class="catetory">openness</view>
        <progress class="ability" color="#FF00FF" percent="{{features.openness.weight * 10}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">emotion</view>
        <progress class="ability" color="#ffff66" percent="{{features.emotion.weight * 10}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">cooperation</view>
        <progress class="ability" color="#10AEFF" percent="{{features.cooperation.weight * 10}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">belief</view>
        <progress class="ability" color="#ff3333" percent="{{features.belief.weight * 10}}" active stroke-width="6" />
      </view>
      <view class="progress-box">
        <view class="catetory">potential</view>
        <progress class="ability" color="#ff3333" percent="{{features.potential.weight * 10}}" active stroke-width="6" />
      </view>
    </view>

  </view>

</view>
