<!--miniprogram/pages/explain/result.wxml-->
<view class="six-bg">
  <view class="box1"></view>
  <view class="box2"></view>
  <view class="box3"></view>
  <view class="canvas-module" animation="{{animationData}}">
      <canvas canvas-id="myCanvas" class="canvas-class"/>
  </view>
  <view class="tips">综合</view>
  <button bindtap="start">开始canvas</button>
  <view class="tips">单项</view>
  <view class="comprohesive">
    <view class="progress-box">
      <view class="catetory">专业</view>
      <progress class="ability" color="#FF00FF" percent="{{features.love.weight * 10}}" active stroke-width="6" />
    </view>
    <view class="progress-box">
      <view class="catetory">外向</view>
      <progress class="ability" color="#ffff66" percent="{{features.life.weight * 10}}" active stroke-width="6" />
    </view>
    <view class="progress-box">
      <view class="catetory">专业的</view>
      <progress class="ability" color="#10AEFF" percent="{{features.social.weight * 10}}" active stroke-width="6" />
    </view>
    <view class="progress-box">
      <view class="catetory">情绪</view>
      <progress class="ability" color="#ff3333" percent="{{features.belief.weight * 10}}" active stroke-width="6" />
    </view>
    <view class="progress-box">
      <view class="catetory">情绪</view>
      <progress class="ability" color="#ff3333" percent="{{features.promising.weight * 10}}" active stroke-width="6" />
    </view>
  </view>

</view>


